filter {
  {
    grok {
      match => { "message" => "(?<timestamp>%{MONTHDAY} %{MONTH} %{YEAR} %{TIME}) %{WORD:hostname} %{DATA:avc_message}" }
    }
    date {
      match => [ "timestamp", "dd MMM yyyy HH:mm:ss" ]
    }
    kv {
      source => "avc_message"
      target => "selinux"
      field_split => " "
      value_split => "="
    }
  }
}